# PharMind Web - Environment Variables Example
# Copy this file to .env.local for local development
# Copy this file to .env.production for production builds

# API Base URL (Backend)
VITE_API_BASE_URL=http://localhost:5209/api

# Application Name
VITE_APP_NAME=PharMind

# Environment (development, staging, production)
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_ANALYTICS=false

# Optional: SignalR Hub URL (if different from API)
# VITE_SIGNALR_HUB_URL=http://localhost:5209/hubs
